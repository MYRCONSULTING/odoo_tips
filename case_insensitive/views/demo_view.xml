<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>
        <record id="demo_model_view_form" model="ir.ui.view">
            <field name="name">demo.model.form</field>
            <field name="model">demo.model</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form >
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group></group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_demo_model" model="ir.actions.act_window">
            <field name="name">Demo Model</field>
            <field name="res_model">demo.model</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="demo_model_menu" name="Demo Model" action="action_demo_model"/>
    </data>
</openerp>